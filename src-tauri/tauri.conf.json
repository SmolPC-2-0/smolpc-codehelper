{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "identifier": "com.smolpc.codehelper",
  "productName": "SmolPC Code Helper",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "SmolPC Code Helper",
        "width": 900,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": ["icons/SmolPC.png", "icons/SmolPC.ico", "icons/SmolPC.icns"]
  }
}
